<!doctype html>

<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width,initial-scale=1.0,user-scalable=no">

  <title>Process Portal - Work</title>

  <link rel="stylesheet" href="{{ pageInfo.rootPath }}/styles/bpm.css" />

</head>

<body>

  <div id="bpm_container">

    <div id="bpm_leftcol">
      <img src="{{ pageInfo.rootPath }}/img/bg-left-menu-top.png" />
      {% block leftmenu %}{% endblock %}
    </div><!-- //bpm_leftcol -->

    <div id="bpm_rightcol">

      <div id="bpm_header">

        <div id="bpm_header_left">
          <img src="{{ pageInfo.rootPath }}/img/bg-header-menu-left.png" />
        </div><!-- //bpm_header -->

        <div id="bpm_header_centre">
          {% block headertext %}{% endblock %}
        </div><!-- //bpm_header -->

        <div id="bpm_header_right">
          Right
        </div><!-- //bpm_header -->

      </div><!-- //bpm_header -->

      <div id="bpm_page">
        {% block page %}{% endblock %}
      </div><!-- //bpm_page -->

    </div><!-- //bpm_rightcol -->

  </div><!-- //bpm_container -->





</body>
</html>
