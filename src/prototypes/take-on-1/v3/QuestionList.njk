{% from "prototypes/take-on-1/data/LU-6785-Questions-json.dta" import questions %}




<table id="basic-table" class="table ">
  <caption class="table__caption">A basic table with a caption</caption>
  <thead class="table__head">
    <tr class="table__row">
      <th scope="col" class="table__header ">Number</th>
      <th scope="col" class="table__header ">Question</th>
      <th scope="col" class="table__header ">Response</th>
      <th scope="col" class="table__header ">Adjusted</th>
      <th scope="col" class="table__header ">Status</th>
      <th scope="col" class="table__header ">Action</th>
    </tr>
  </thead>
  <tbody class="table__body">
    

    {% for q in questions %}
        {% if q.Status == "Validations" %}

            <tr class="table__row" bgcolor="pink">

        {% elif  q.Status == "Edited" %}
        
            <tr class="table__row" bgcolor="lightblue">

        {% else %}

            <tr class="table__row">
            

        {% endif %}
            <td class="table__cell ">
                {{ q.Number }} 
            </td>
            <td class="table__cell ">
                {{ q.Question }} 
            </td>

            <td class="table__cell "> 
                {{ q.Response }} 
            </td class="table__cell ">

            <td class="table__cell "> 
                {{ q.Adjusted }} 
            </td class="table__cell ">

            <td class="table__cell "> 
                

                {% if q.Status == "Edited" %}

                    <span class='status status--info'>{{ q.Status }}</span>

                {% endif %}

                {% if q.Status == "Validations" %}

                    <span class='status status--error'>{{ q.ValidationMessages }}</span>

                {% endif %}



            </td class="table__cell ">

            <td class="table__cell "> 
                
                <a href="http://localhost:3333/prototypes/take-on-1/v3/ViewForm.html" role="button" class="btn btn--secondary btn--small"><span class="btn__inner">View</span></a>

            </td class="table__cell ">

        </tr>
    {% endfor %}
  </tbody>
</table>

